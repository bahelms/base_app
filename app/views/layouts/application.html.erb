<!DOCTYPE html>
<html>
<head>
  <title>MtgWebGame</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div class="container">
    <% flash.each do |key, value| %>
      <%= flash_content(key, value) %>
    <% end %>
    <%= yield %>
  </div>

</body>
</html>
